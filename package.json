{
  "name": "moverz-analytics",
  "version": "1.0.0",
  "description": "Système analytics complet pour le réseau Moverz (11 sites)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch etl/scheduler.ts",
    "build": "tsc",
    "start": "node dist/etl/gsc/fetch-simple.js",
    "run:once": "tsx etl/gsc/fetch-simple.ts",
    "etl:gsc": "tsx etl/gsc/fetch.ts",
    "etl:gsc-simple": "tsx etl/gsc/fetch-simple.ts",
    "etl:ga4": "tsx etl/ga4/export.ts",
    "etl:leads": "tsx etl/leads/sync.ts",
    "etl:vitals": "tsx etl/web-vitals/aggregate.ts",
    "backfill": "tsx scripts/backfill.ts",
    "split-tables": "tsx scripts/split-tables.ts",
    "db:migrate": "tsx db/migrations/run.ts",
    "db:seed": "tsx db/seed_sites.ts",
    "dashboard:dev": "cd dashboard && npm run dev",
    "dashboard:build": "cd dashboard && npm run build",
    "agents:run": "tsx agents/core/orchestrator.ts",
    "test": "vitest",
    "test:events": "tsx scripts/test/test-ga4-events.ts",
    "test:bigquery": "tsx scripts/test/validate-bigquery.ts",
    "setup:ga4": "bash scripts/setup/init-ga4.sh",
    "setup:bigquery": "bash scripts/setup/init-bigquery.sh",
    "deploy:tracking": "bash scripts/deploy/sync-tracking-to-sites.sh",
    "bq:init": "tsx scripts/init-bigquery.ts",
    "bq:check": "tsx scripts/check-bigquery.ts",
    "bq:query": "tsx scripts/query-bigquery.ts"
  },
  "dependencies": {
    "@google-analytics/data": "^4.7.0",
    "@google-cloud/bigquery": "^7.3.0",
    "googleapis": "^129.0.0",
    "pg": "^8.11.3",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "openai": "^4.20.1",
    "node-cron": "^3.0.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "prettier": "^3.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "analytics",
    "bigquery",
    "google-analytics",
    "search-console",
    "ai-agents",
    "dashboard"
  ],
  "author": "Moverz Team",
  "license": "PROPRIETARY"
}

